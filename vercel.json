{
    "builds": [{
        "src": "package.json", // as stated here in `vercel.json`
        "use": "vercel-sapper",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.10" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "config/wsgi.py"
        }
    ]
}
